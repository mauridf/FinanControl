<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="FinanControl.App.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:FinanControl.App"
    Title="FinanControl.App">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
    Title="Contas"
    Icon="account_balance.png"
    ContentTemplate="{DataTemplate views:ContasPage}"
    Route="ContasPage" />

</Shell>
